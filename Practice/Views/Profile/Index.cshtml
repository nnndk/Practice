@model Dictionary<string, string>
@{
    ViewData["Title"] = "Profile";
}

<div class="container">
    <div class="row align-middle">
        <div class="col"></div>
        <div class="col">
            <div class="row">
                <div class="col mt-4 login-font">
                    <h1 class="text-center mb-5">Профиль</h1>
                    <table class="table table-striped" ">
                        <tbody>
                            @foreach (var item in Model)
                            {
                            <tr>
                                <td>@item.Key</td>
                                <td>@item.Value</td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="text-end">
                <a class="btn btn-dark" asp-controller="Login" asp-action="Logout">
                    Выйти
                </a>
            </div>
        </div>
        <div class="col"></div>
    </div>
</div>
