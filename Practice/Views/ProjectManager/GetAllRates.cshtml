@model List<СтавкиСотрудников>
@{
    ViewData["Title"] = "GetAllRates";
}

<div class="row">
    <h1 class="mt-3 mb-3">Менеджер проекта</h1>
</div>

<div class="row align-middle">
    <div class="col-2"></div>

    <div class="col-8">
        <table class="table table-al table-striped align-middle">
            <thead>
                <tr>
                    <th scope="col">Дата начала действия ставки</th>
                    <th scope="col">Ставка</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.ДатаНачалаДействияСтавки</td>
                        <td>@item.Ставка</td>
                    </tr>
                }
            </tbody>
        </table>

        <div class="text-end">
            <a asp-controller="ProjectManager" asp-action="UpdateDeveloper" asp-route-id=@ViewBag.Id asp-route-projectid=@ViewBag.ProjectId class="btn btn-secondary mx-3" style="width:150px">
                Назад
            </a>
        </div>
    </div>

    <div class="col-2"></div>
</div>
